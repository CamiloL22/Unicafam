//Calcular e imprimir la tabla de multiplicar de un n√∫mero cualquiera. Imprimir el
//multiplicando, el multiplicador y el producto.
import java.util.Scanner;
public class Ejercicio7 {
    public static void main(String[] args) {
        Scanner consola = new Scanner(System.in);
        
        System.out.println("Ingrese un numero");
        int multiplicando = consola.nextInt();
        int multiplicador = 1;
        
        while (multiplicador <= 10){
            int producto;
            producto = multiplicador*multiplicando;
            
            System.out.println(multiplicando + " x " + multiplicador + " = " + producto);
            multiplicador++;
        }
    }
}
